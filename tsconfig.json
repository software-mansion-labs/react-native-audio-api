{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "preserveSymlinks": true,
    "target": "es6",
    "module": "NodeNext",
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "NodeNext",
    "moduleSuffixes": [".ios", ".android", ".native", ".web", ""],
    "lib": ["es6", "dom", "ES2021"],
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
